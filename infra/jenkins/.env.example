# Jenkins Environment Variables
# Copy this file to .env and fill in your values

# GitHub Configuration
GITHUB_TOKEN=your_github_token

# Docker Registry Configuration
# For GitHub Container Registry (ghcr.io):
DOCKER_REGISTRY_USER=your_docker_registry_username
DOCKER_REGISTRY_PASS=your_docker_registry_password

# Azure Service Principal Configuration
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_TENANT_ID=your_azure_tenant_id

# Kubernetes Configuration (base64 encoded kubeconfig)
# To generate: [Convert]::ToBase64String([System.IO.File]::ReadAllBytes("$HOME\.kube\config"))
KUBECONFIG_BASE64=your-kubeconfig_base64_here

# Jenkins Admin Credentials (configured in casc.yaml)
JENKINS_ADMIN_USER=your_jenkins_admin_user
JENKINS_ADMIN_PASSWORD=your_jenkins_admin_password

# SonarQube Configuration
SONAR_ADMIN_PASSWORD=your_sonar_admin_password
SONAR_DB_PASSWORD=your_sonar_db_password

# PostgreSQL Configuration (for SonarQube)
POSTGRES_USER=sonar
POSTGRES_PASSWORD=sonar
POSTGRES_DB=sonar
